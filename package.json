{
  "name": "devjobs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "db:up": "docker-compose up",
    "db:down": "docker-compose down",
    "db:mysql": "winpty docker exec -it devjobs_db mysql -u root -ppassword",
    "db:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate"
  },
  "dependencies": {
    "cookie-parser": "1.4.6",
    "debug": "~2.6.9",
    "dotenv": "^15.0.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "sequelize": "^6.16.2"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.4.1"
  }
}
